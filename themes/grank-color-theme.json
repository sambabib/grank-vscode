{
  "name": "Grank",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4",
    "editorCursor.foreground": "#C586C0",
    "editor.lineHighlightBackground": "#2D2D2D",
    "editor.selectionBackground": "#4EC9B033",
    "editor.inactiveSelectionBackground": "#4EC9B019",
    "editorIndentGuide.background1": "#2D2D2D",
    "editorIndentGuide.activeBackground1": "#4EC9B0",
    "activityBar.background": "#1E1E1E",
    "activityBar.foreground": "#4EC9B0",
    "activityBar.inactiveForeground": "#6FCFB080",
    "activityBarBadge.background": "#C586C0",
    "activityBarBadge.foreground": "#FFFFFF",
    "sideBar.background": "#1E1E1E",
    "sideBar.foreground": "#D4D4D4",
    "sideBarTitle.foreground": "#4EC9B0",
    "sideBarSectionHeader.background": "#2D2D2D",
    "sideBarSectionHeader.foreground": "#D4D4D4",
    "list.activeSelectionBackground": "#4EC9B033",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#2D2D2D",
    "list.hoverBackground": "#2D2D2D",
    "list.highlightForeground": "#4EC9B0",
    "titleBar.activeBackground": "#1E1E1E",
    "titleBar.activeForeground": "#D4D4D4",
    "titleBar.inactiveBackground": "#1E1E1E",
    "titleBar.inactiveForeground": "#D4D4D480",
    "statusBar.background": "#1E1E1E",
    "statusBar.foreground": "#4EC9B0",
    "statusBar.noFolderBackground": "#1E1E1E",
    "statusBar.debuggingBackground": "#C586C0",
    "statusBar.debuggingForeground": "#FFFFFF",
    "tab.activeBackground": "#1E1E1E",
    "tab.inactiveBackground": "#2D2D2D",
    "tab.activeForeground": "#D4D4D4",
    "tab.inactiveForeground": "#8A8A8A",
    "tab.activeBorderTop": "#4EC9B0",
    "button.background": "#4EC9B0",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#6FCFB0",
    "dropdown.background": "#1E1E1E",
    "dropdown.foreground": "#D4D4D4",
    "dropdown.border": "#2D2D2D",
    "input.background": "#2D2D2D",
    "input.foreground": "#D4D4D4",
    "input.border": "#2D2D2D",
    "inputOption.activeBorder": "#4EC9B0",
    "panel.background": "#1E1E1E",
    "panel.border": "#2D2D2D",
    "panelTitle.activeForeground": "#4EC9B0",
    "panelTitle.inactiveForeground": "#D4D4D4",
    "badge.background": "#4EC9B0",
    "badge.foreground": "#FFFFFF",
    "terminal.background": "#1E1E1E",
    "terminal.foreground": "#D4D4D4",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBrightBlack": "#2D2D2D",
    "terminal.ansiRed": "#F44747",
    "terminal.ansiBrightRed": "#FF6B6B",
    "terminal.ansiGreen": "#4EC9B0",
    "terminal.ansiBrightGreen": "#6FCFB0",
    "terminal.ansiYellow": "#CE9178",
    "terminal.ansiBrightYellow": "#DCDCAA",
    "terminal.ansiBlue": "#569CD6",
    "terminal.ansiBrightBlue": "#6FB9FF",
    "terminal.ansiMagenta": "#C586C0",
    "terminal.ansiBrightMagenta": "#D7BAFF",
    "terminal.ansiCyan": "#9CDCFE",
    "terminal.ansiBrightCyan": "#B5DCFE",
    "terminal.ansiWhite": "#D4D4D4",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Methods",
      "scope": ["meta.method.declaration", "meta.method-call"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Properties",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Constants",
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#4FC1FF"
      }
    },
    {
      "name": "Imports and packages",
      "scope": [
        "entity.name.import",
        "entity.name.package",
        "keyword.control.import",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "name": "Language variables",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.inline.any.html"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "CSS IDs",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#CE9178",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "italic"
      }
    }
  ]
}
